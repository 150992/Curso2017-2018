@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

:catalog
	a dcat:Catalog ;
	dct:title "Catalogo de actividades realizadas en bibliotecas y bibliobuses de Madrid"@es,
            "Catalogue for activities done in libraries of Madrid"@en;
	dct:source <http://datos.madrid.es> ;
  dct:license rdflicense: CC-BY-NC ;
	dct:publisher <http://github.com/javig13>;
	dct:publisher <http://github.com/Hiro-xyz>;
	dct:publisher <http://github.com/mordisquitos42>;
	dct:publisher <http://github.com/Forcada>;
  dct:publisher <http://github.com/LaiKash>;
	dct:language <http://www.lexvo.org/page/iso639-3/eng> 
              ,<http://www.lexvo.org/page/iso639-3/spa> ;
	dcat:dataset :Activities, Libraries: ;  .

# Activities
:Activities
	a dcat:Dataset ;
	dct:title "Actividades realizadas en bibliotecas y bibliobuses de Madrid."@es
           ,"Activities done in libraries of Madrid"@en;
	dcat:keyword "Activities"@en "Actividades"@es ;
	dct:license rdflicense: CC-BY-NC;
  .
  
# Libraries
:Libraries
	a dcat:Dataset ;
	dct:title "Bibliotecas y bibliobuses de Madrid."@es
           ,"Libraries in Madrid"@en;
	dcat:keyword "Libraries"@en "Bibliotecas"@es ;
	dct:license rdflicense: CC-BY-NC;
