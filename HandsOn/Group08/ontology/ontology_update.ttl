@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix vevent: <http://www.w3.org/2002/12/cal#>
@prefix : <http://www.semanticweb.es/group08/ontology#> .
@base <http://www.semanticweb.es/group08/ontology> .
 
 
 : rdf:type owl:Ontology . 
 
 #################################################################
 #    Object Properties
 ################################################################# 
 
 ###  http://www.w3.org/2002/12/cal#location
 vevent:location	rdf:type owl:ObjectProperty ;
            rdfs:domain :activity ;
            rdfs:range :library .
 
 
 #################################################################
 #    Data properties
 #################################################################
 
 ###  https://www.w3.org/2006/vcard/ns#given-name
 vcard:given-name rdf:type owl:DatatypeProperty ;
          rdfs:domain :library ;
          rdfs:range xsd:string .
 
 
 ###  http://www.semanticweb.es/group08/ontology#hasSchedule
 :hasSchedule rdf:type owl:DatatypeProperty ;
              rdfs:domain :library ;
              rdfs:range xsd:string .
 
 
 ###  https://www.w3.org/2006/vcard/ns#hasAddress
 vcard:hasAddress rdf:type owl:DatatypeProperty ;
             rdfs:domain :library ;
             rdfs:range xsd:string .
 
 
 ###  https://www.w3.org/2006/vcard/ns#hasTelephone
 vcard:hasTelephone rdf:type owl:DatatypeProperty ;
               rdfs:domain :library ;
               rdfs:range xsd:string .
 
 ###  https://www.w3.org/2006/vcard/ns#hasEmail
 vcard:hasEmail rdf:type owl:DatatypeProperty ;
               rdfs:domain :library ;
               rdfs:range xsd:string .
  

 ###  https://www.w3.org/2006/vcard/ns#fn
 vcard:fn rdf:type owl:DatatypeProperty ;
           rdfs:domain :activity ;
           rdfs:range xsd:string .
 
 
 ###  http://www.w3.org/2002/12/cal#byday
 vevent:byday rdf:type owl:DatatypeProperty ;
          rdfs:domain :activity ;
          rdfs:range xsd:string .
					  
					  
 ###  http://www.w3.org/2002/12/cal#dtstart
 vevent:ddtstart rdf:type owl:DatatypeProperty ;
            rdfs:domain :activity ;
            rdfs:range xsd:string .

 ###  http://www.w3.org/2002/12/cal#dtend
 vevent:dtend rdf:type owl:DatatypeProperty ;
          rdfs:domain :activity ;
          rdfs:range xsd:string .

 ###  https://www.w3.org/2006/vcard/ns#hasUrl
 vevent:hasUrl rdf:type owl:DatatypeProperty ;
	  rdfs:domain :activity ;
	  rdfs:range xsd:string .
					  
 
 #################################################################
 #    Classes
 #################################################################
 
 ###  http://www.semanticweb.es/group08/ontology#library
 :library rdf:type owl:Class .
 
 
 ###  http://www.semanticweb.es/group08/ontology#activity
 :activity rdf:type owl:Class . 
